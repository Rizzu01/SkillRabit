{
  "functions": {
    "api/index.php": {
       "runtime": "vercel-php@0.7.4"
    }
  },
  {
  "builds": [
    { "src": "api/**/*.php", "use": "@vercel/php" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
}
